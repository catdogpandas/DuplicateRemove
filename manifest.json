{
    "name": "RemoveDuplicate",
    "description": "Remove duplicate results from baidu.com",
    "version": "2.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.baidu.com/*"
            ],
            "run_at": "document_idle",
            "js": [
                "/scripts/content.js"
            ]
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            "16": "/images/icon16.png",
            "32": "/images/icon32.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    },

    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    }
}